"UnlitGeneric"
{
	"$basetexture" "models\kirillian\zr_rogue_dome_1/br_fog"

	"$translucent" 1

	"$fade_start" 9500.0		
	//Fade out would begin at ($fade_start) units and end at ($fade_start/var2) units with player distance being inverse to alpha value
	
	"$temp_fade_start" 0.0
	"$var" 1.0
	"$var2" 0.9
	"$var3" 2000.0
	"$var4" 400.0
	"$vartemp1" 0.0
	"$distance" 0.0
	"$temp" 0.0
	"$temp_clamped" 0.0
	"$temp2" 0.0
	"$temp_speedGet" 0.0
	"$alpha" "255.0"
	"$defaultalpha" "255.0"
	
	"$alpha_unclamped" 	0.0
	"$frameSet" 		0
	"$var4multi" 		10.0 //max textures
	"$var5multi" 		5.0 //more agressive

	"Proxies"
	{
		"PlayerProximity"
		{
			"scale" 1.0
			"resultVar" "$distance"
		}

		"Alpha"
		{
			
		}
		"Divide"
		{
			"srcVar1" "$alpha"
			"srcVar2" "$var4"
			"resultVar" "$vartemp1"
		}
	//	"Clamp"
	//	{
	//		"min" 0.25
	//		"max" 0.25
	//		"srcVar1" "$vartemp1"
	//		"resultVar" "$vartemp1"
	//	}
		"Multiply"
		{
			"srcVar1" "$fade_start"
			"srcVar2" "$vartemp1"
			"resultVar" "$temp_fade_start"
		}
		"Divide"
		{
			"srcVar1" "$distance"
			"srcVar2" "$temp_fade_start"
			"resultVar" "$temp"
		}
		"Multiply"
		{
			"srcVar1" "$alpha_unclamped"
			"srcVar2" "$var5multi"
			"resultVar" "$alpha_unclamped"
		}
		"Clamp"
		{
			"min" 0.0
			"max" "$var3"
			"srcVar1" "$temp"
			"resultVar" "$temp_clamped"
		}
		"Divide"
		{
			"srcVar1" "$var"
			"srcVar2" "$temp_clamped"
			"resultVar" "$temp2"
		}
		"Subtract" //this is used so that the block actually becomes fully transparent at ($temp_fade_start / $var2) units
		{
			"srcVar1" "$temp2"
			"srcVar2" "$var2"
			"resultVar" "$alpha_unclamped"
		}
		"Multiply"
		{
			"srcVar1" "$alpha_unclamped"
			"srcVar2" "$var5multi"
			"resultVar" "$alpha_unclamped"
		}

		"Clamp"
		{
			"min" "0.0"
			"max" "1.0"
			"srcVar1" "$alpha_unclamped"
			"resultVar" "$alpha_unclamped"
		}
		//current 10 frames.
		"Multiply"
		{
			"srcVar1" "$alpha_unclamped"
			"srcVar2" "$var4multi"
			"resultVar" "$frame"
		}
	}
}
